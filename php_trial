    <ul>
        <?php
        for ($i=0; $i < count($posts); $i++) {
            $single_data = $posts[$i]; 
            ?>
            <li><?php echo $single_data; ?> Data 
                <ul>
                    <?php
                    for ($j=0; $j < count($single_data); $j++) {
                        $data_posts = $single_data[$j];  
                    ?>
                    <li>
                        <?php echo $data_posts ?> 
                    
                        <ul>
                            <?php for ($k=0; $k < count($data_posts); $k++) {
                            $single_post = $data_posts[$k];  
                            ?>
                            <li>
                                <?php echo $single_post['title']; ?>
                            </li>
                            <?php } 
                            ?>
                        </ul>
                    </li>
                    <?php }
                    ?>
                </ul>
            </li>
        <?php }  
        ?>
    </ul>